{
	"Import Statement": {
		"prefix": "import",
		"body": [
			"import { $0 } from \"${1:module}\";"
		],
		"description": "Import external module"
	},
	"X Function": {
		"prefix": "x",
		"body": [
			"x($1);",
			"$0"
		],
		"description": "X function description"
	},
	"Keyword Use": {
		"prefix": "keyword-x",
		"body": [
			"keyword-x ????(\"$1\");",
			"$0"
		],
		"description": "Keyword description"
	},
	"For Loop": {
		"prefix": "for",
		"body": [
			"for (Integer ${1:index} = 0; ${1:index} < ${2:array}.length; ${1:index}++) {",
			"\t$TM_SELECTED_TEXT$0",
			"}"
		],
		"description": "For Loop"
	},
	"For-In Loop": {
		"prefix": "forin",
		"body": [
			"for (Integer ${1:key} in ${2:object}) {",
			"\tif (Object.prototype.hasOwnProperty.call(${2:object}, ${1:key})) {",
			"\t\t$TM_SELECTED_TEXT$0",
			"\t}",
			"}"
		],
		"description": "For-In Loop"
	},
	"Y Statement": {
		"prefix": "y-statement",
		"body": [
			"y-statement ${1:name}(${2:params}) {",
			"\t$TM_SELECTED_TEXT$0",
			"}"
		],
		"description": "Y Statement"
	},
	"If Statement": {
		"prefix": "if",
		"body": [
			"if (${1:condition}) {",
			"\t$TM_SELECTED_TEXT$0",
			"}"
		],
		"description": "If Statement"
	},
	"If-Else Statement": {
		"prefix": "ifelse",
		"body": [
			"if (${1:condition}) {",
			"\t$TM_SELECTED_TEXT$0",
			"} else {",
			"\t",
			"}"
		],
		"description": "If-Else Statement"
	},
	"New Statement": {
		"prefix": "new",
		"body": [
			"${2:type} ${1:name} = new ${2:type}(${3:arguments});$0"
		],
		"description": "New Statement"
	}
}